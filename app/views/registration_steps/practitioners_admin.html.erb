<div class="app-container partner-wrapper clinic-wrapper">
  <div class="page">
    <div class="tabs-container"
      ><ul class="tabs">
        <li class=""><a class="btn transparent" href="/registration_steps/company"><span class="flex">Klinikoplysninger</span></a></li>
        <li class=""><a class="btn transparent" href="/registration_steps/practitioners"><span class="flex">Behandlere</span></a></li>
      </ul>
    </div>

   <div class="container">
	    <div class="row">
        <div class="content-container">
            <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 bhoechie-tab-menu">
              <div class="list-group">
                <a href="#" class="list-group-item active text-center">
                  <h4 class="glyphicon glyphicon-pencil"></h4><br/>GENEREL INFO
                </a>
                <a href="#" class="list-group-item text-center">
                  <h4 class="glyphicon glyphicon-road"></h4><br/>PROFESSIONER, YDELSER OG SPECIALER
                </a>
                <a href="#" class="list-group-item text-center">
                  <h4 class="glyphicon glyphicon-award"></h4><br/>UDDANNELSER, MEDLEMSKABER OG UDMÆRKELSER
                </a>
              </div>
            </div>
            <div class="col-lg-9 col-md-9 col-sm-9 col-xs-9 bhoechie-tab">         
               
                <!-- GENEREL INFO section -->
            <div class="bhoechie-tab-content active">
            <center>
              <div class="content">
                <div class="content practitioner">
                  <h2 class="page-title">Generel information</h2>
                  <%= form_for @user, url: wizard_path, method: :put do |f| %>
                    <div class="basic-section">
                      <div class="image-field field profile-picture">
                        <div class="image image-upload" style="background-image: url(&quot;&quot;);">
                          <a class="overlay">
                            <i class="material-icons icon ">add</i>
                          </a>
                          <span class="text-container">Tilføj billede</span>
                          <input type="file" accept="image/*">
                        </div>
                      </div>
                      <div class="info-group">
                        <div class="field-group">
                          <div class="field text-field">
                            <%= f.text_field :practitioner_first_name, autofocus: true, autocomplete: "Fornavn", placeholder: "Fornavn" %>
                          </div>
                          <div class="field text-field">
                            <%= f.text_field :practitioner_last_name, autofocus: true, autocomplete: "Efternavn", placeholder: "Efternavn" %>
                          </div>
                        </div>
                        <div class="field-group radio-group">
                          <label>Køn</label>
                          <div class="field checkbox-field ">
                            <span class="checkbox">
                              <input type="radio" id="jovckz" name="gender" value="on"><label for="jovckz">Mand</label>
                            </span>
                          </div>
                          <div class="field checkbox-field ">
                            <span class="checkbox">
                              <input type="radio" id="zuydax" name="gender" value="on"><label for="zuydax">Kvinde</label>
                            </span>
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="about-section">
                      <div class="field text-field">
                        <%= f.text_area :practitioner_description, :rows => 5, autofocus: true, autocomplete: "Beskrivelse af behandler", placeholder: "Beskrivelse af behandler" %>
                      </div>
                      <div class="field checkbox-field ">
                        <span class="checkbox">
                          <input type="checkbox" id="yryker" value="on"><label for="yryker">Overenskomst med den offentlige sygesikring</label>
                        </span>
                      </div>
                    </div>
                    <div class="field-group contact-section">
                      <div class="field text-field">
                        <%= f.text_field :practitioner_mail, autofocus: true, autocomplete: "E-mail", placeholder: "E-mail" %>
                      </div>
                      <div class="field text-field">
                        <%= f.text_field :practitioner_phone, autofocus: true, autocomplete: "Tlf. nr.", placeholder: "Tlf. nr." %>
                      </div>
                      <div class="field text-field">
                        <%= f.text_field :practitioner_website, autofocus: true, autocomplete: "Hjemmeside", placeholder: "Hjemmeside" %>
                      </div>
                    </div>
                    <div class="btn-container">
                      <%= f.submit "Gem", :class => 'btn blue'  %> 
                  <% end %>
                    </div>
                </div>
              </div>
            </center>
            </div>


                <!-- PROFESSIONER, YDELSER OG SPECIALER section -->
            <div class="bhoechie-tab-content">
            <center>
              <div class="content">
                <div class="content practitioner">
                  <%= form_for @user, url: wizard_path, method: :put do |f| %>
                    <div class="specializations-section">
                    <h2 class="page-title">Professioner</h2>
                      <div class="field select-field ">
                        <div class="Select Select--multi is-searchable has-value">
                          <div class="Select-control">
                            <span class="Select-multi-value-wrapper" id="react-select-4--value">
                              <div class="Select-value">
                                <span class="Select-value-icon" aria-hidden="true"></span>
                                <span class="Select-value-label" role="option" aria-selected="true" id="react-select-4--value-0">
                                  <span class="Select-aria-only">&nbsp;</span>
                                </span>
                              </div>
                              <div class="Select-value">
                                <span class="Select-value-icon" aria-hidden="true"></span>
                                <span class="Select-value-label" role="option" aria-selected="true" id="react-select-4--value-1">
                                  <span class="Select-aria-only">&nbsp;</span>
                                </span>
                              </div>
                              <div class="Select-input" style="display: inline-block;">
                                <input role="combobox" aria-expanded="false" aria-owns="" aria-haspopup="false" aria-activedescendant="react-select-4--value" value="" style="width: 5px; box-sizing: content-box;">
                                <div style="position: absolute; top: 0px; left: 0px; visibility: hidden; height: 0px; overflow: scroll; white-space: pre;"></div>
                              </div>
                            </span>
                            <span class="Select-arrow-zone">
                              <span class="Select-arrow"></span>
                            </span>
                          </div>
                        </div>
                      </div>
                      <div class="content">
                        <p>Mangler der en profession på listen? Så skriv til os på&nbsp;<a href="mailto:info@betterwing.dk?Subject=Forslag%20til%20ny%20profession">info@betterwing.dk</a></p>
                      </div>
                    </div>
                    <div class="symptoms-section">
                      <h2 class="page-title">Specialer</h2>
                        <div class="field select-field ">
                          <div class="Select Select--multi is-searchable has-value">
                            <div class="Select-control">
                              <span class="Select-multi-value-wrapper" id="react-select-5--value">
                                <div class="Select-value">
                                  <span class="Select-value-icon" aria-hidden="true"></span>
                                  <span class="Select-value-label" role="option" aria-selected="true" id="react-select-5--value-0">
                                    <span class="Select-aria-only">&nbsp;</span>
                                  </span>
                                </div>
                                <div class="Select-value">
                                  <span class="Select-value-icon" aria-hidden="true"></span>
                                  <span class="Select-value-label" role="option" aria-selected="true" id="react-select-5--value-1">
                                    <span class="Select-aria-only">&nbsp;</span>
                                  </span>
                                </div>
                                <div class="Select-input" style="display: inline-block;">
                                  <input role="combobox" aria-expanded="false" aria-owns="" aria-haspopup="false" aria-activedescendant="react-select-5--value" value="" style="width: 5px; box-sizing: content-box;">
                                  <div style="position: absolute; top: 0px; left: 0px; visibility: hidden; height: 0px; overflow: scroll; white-space: pre;"></div>
                                </div>
                              </span>
                              <span class="Select-arrow-zone">
                                <span class="Select-arrow"></span>
                              </span>
                            </div>
                          </div>
                        </div>
                        <div class="content">
                          <p>Mangler der et speciale på listen? Så skriv til os på&nbsp;<a href="mailto:info@betterwing.dk?Subject=Forslag%20til%20nyt%20speciale">info@betterwing.dk</a></p>
                        </div>
                    </div>
                    <div class="services-section">
                      <h2 class="page-title">Ydelser</h2>
                      <div class="field-group service">
                        <div class="btn-container delete">
                          <button class="btn transparent" type="button">
                            <span class="flex"><i class="material-icons icon ">close</i></span>
                          </button>
                        </div>
                        <div class="field text-field name has-value">
                          <input type="text" value="dad" id="gxpo1a" placeholder="Navn på behandling" title="Navn på behandling" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
                        </div>
                        <div class="field text-field description has-value">
                          <textarea id="niwc4b" rows="1" placeholder="Beskrivelse af behandling">dddas</textarea>
                        </div>
                        <div class="field text-field  has-value">
                          <input type="number" min="15" max="720" value="33" id="t8qq1x" placeholder="Minutter" title="Minutter">
                        </div>
                        <div class="field text-field  has-value">
                          <input type="number" value="33" id="7rbibg" placeholder="Pris (DKK)" title="Pris (DKK)">
                        </div>
                      </div>
                      <div class="btn-container">
                        <button class="btn transparent" type="button">
                          <span class="flex">
                            <i class="material-icons icon ">add</i>
                            <span class="text">Tilføj flere ydelser</span>
                          </span>
                        </button>
                      </div>
                    </div>
                    <div class="btn-container">
                      <%= f.submit "Gem", :class => 'btn blue'  %> 
                    </div>
                  <% end %>
                </div>
              </div>         
            </center>
            </div>
    
               
                  <!-- UDDANNELSER, MEDLEMSKABER OG UDMÆRKELSER -->
            <div class="bhoechie-tab-content">
            <center>
              <div class="content">
                <div class="content">
                  <%= form_for @user, url: wizard_path, method: :put do |f| %>
                    <div class="education-section">
                      <h2 class="page-title">Uddannelse</h2>
                      <div class="field-group education">
                        <div class="btn-container delete">
                          <button class="btn transparent" type="button">
                            <span class="flex">
                              <i class="material-icons icon ">close</i>
                            </span>
                          </button>
                        </div>
                        <div class="field text-field qualification has-value">
                          <input type="text" value="asa" id="zqzxbq" placeholder="Uddannelse" title="Uddannelse" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%;">
                        </div>
                        <div class="field text-field  has-value">
                          <input type="text" value="sas" id="bri39c" placeholder="Uddannelsessted" title="Uddannelsessted">
                        </div>
                        <div class="field text-field  has-value">
                          <input type="number" min="1901" max="2017" value="1999" id="0b4ip1" placeholder="År (YYYY)" title="År (YYYY)">
                        </div>
                      </div>
                      <div class="btn-container">
                        <button class="btn transparent" type="button">
                          <span class="flex">
                            <i class="material-icons icon ">add</i>
                          <span class="text">Tilføj flere uddannelser</span>
                          </span>
                        </button>
                      </div>
                    </div>
                    <div class="memberships-section">
                      <h2 class="page-title">Medlemskaber</h2>
                        <div class="field-group membership">
                          <div class="btn-container delete">
                            <button class="btn transparent" type="button">
                              <span class="flex">
                                <i class="material-icons icon ">close</i>
                              </span>
                            </button>
                          </div>
                          <div class="field select-field ">
                            <div class="Select Select--single is-searchable has-value">
                              <div class="Select-control">
                                <span class="Select-multi-value-wrapper" id="react-select-6--value">
                                  <div class="Select-value">
                                    <span class="Select-value-label" role="option" aria-selected="true" id="react-select-6--value-item">DAU - Dansk Akupunktur Union</span>
                                  </div>
                                  <div class="Select-input" style="display: inline-block;">
                                    <input role="combobox" aria-expanded="false" aria-owns="" aria-haspopup="false" aria-activedescendant="react-select-6--value" value="" style="width: 5px; box-sizing: content-box;">
                                    <div style="position: absolute; top: 0px; left: 0px; visibility: hidden; height: 0px; overflow: scroll; white-space: pre;"></div>
                                  </div>
                                </span>
                                <span class="Select-arrow-zone">
                                  <span class="Select-arrow"></span>
                                </span>
                              </div>
                            </div>
                          </div>
                        </div>
                        <div class="btn-container">
                          <button class="btn transparent" type="button">
                            <span class="flex">
                              <i class="material-icons icon ">add</i> 
                              <span class="text">Tilføj flere medlemskaber</span>
                            </span>
                          </button>
                        </div>
                    </div>
                    <div class="awards-section">
                      <h2 class="page-title">Udmærkelser</h2>
                        <div class="field-group award">
                          <div class="btn-container delete">
                            <button class="btn transparent" type="button">
                              <span class="flex">
                                <i class="material-icons icon ">close</i>
                              </span>
                            </button>
                          </div>
                          <div class="field text-field name has-value">
                            <input type="text" value="3rer" id="h8sh8o" placeholder="Udmærkelse" title="Udmærkelse">
                          </div>
                          <div class="field text-field  has-value">
                            <input type="number" min="1901" max="2017" value="1999" id="488jt6" placeholder="Årstal (YYYY)" title="Årstal (YYYY)">
                          </div>
                        </div>
                        <div class="btn-container">
                          <button class="btn transparent" type="button">
                            <span class="flex">
                              <i class="material-icons icon ">add</i>
                              <span class="text">Tilføj flere udmærkelser</span>
                            </span>
                          </button>
                        </div>
                    </div>
                    <div class="btn-container">
                      <%= f.submit "Gem", :class => 'btn blue'  %> 
                    </div>
                  <% end %>
                </div>
              </div>         
            </center>
            </div>
            </div>
        </div>
      </div>
  </div>
  </div>
</div>